@page
@model MyApp.Namespace.EliminarRevisionModel
@{
}
<div class="justify-content-center text-center">
    <h1>Eliminara la siguiente factura</h1>
    <hr><br>
    <div class="container">
  <div class="row">
    <div class="col" ><strong>Numero de Revision</strong></div>
    <div class="col">@Html.DisplayFor(m => m.Revisiones.NoRevision)</div>
        <div class="w-100"></div>
    <!--<div class="col"><strong>Placa de Vehiculo</strong></div>
    <div class="col"> Html.DisplayFor(m => m.Revisiones.NoPlaca)</div>
        <div class="w-100"></div>-->
    <div class="col"><strong>Tipo de Mantenimiento</strong></div>
    <div class="col">@Html.DisplayFor(m => m.Revisiones.TipoMantenimiento)</div>
        <div class="w-100"></div>
    <div class="col"><strong>Fecha de Revision</strong></div>
    <div class="col">@Html.DisplayFor(m => m.Revisiones.FechaRevision)</div>
        <div class="w-100"></div>
    <div class="col"><strong>Estado del Filtro</strong></div>
    <div class="col">@Html.DisplayFor(m => m.Revisiones.EstadoFiltro)</div>
    <div class="w-100"></div>
    <div class="col"><strong>Repuesto</strong></div>
    <div class="col">@Html.DisplayFor(m => m.Revisiones.Repuesto)</div>
    <div class="w-100"></div>
    <div class="col"><strong>Estado del Aceite</strong></div>
    <div class="col">@Html.DisplayFor(m => m.Revisiones.EstadoAceite)</div>
  </div><br>
    <hr>
    <form method="post" >
    <input type="hidden"  asp-for="Revisiones.Id_revision"/>    
    <input type="submit" value="Confirmar" class="btn btn-danger" asp-page="/Eliminar/OnPost"/>    
    </form>
</div>
<div class="justify-content-center text-left"><a asp-page="./Listar">Ir a Gestion de Revisiones</a></div>