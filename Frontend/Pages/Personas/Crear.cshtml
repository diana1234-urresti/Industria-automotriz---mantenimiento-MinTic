@page
@model MyApp.Namespace.CrearModel
@{
}
<div class="justify-content-center text-center">
    <h1>Crear Persona</h1>
    <br>
    <hr>
 </div>
<form method="post">
  <table class="table-hover" width="600" height="300"  align="center" >
    <tr>
      <td width="260">Nombres</td>
      <td width="340">
       <input asp-for="Personas.Nombre" class="form-control" size="35" />
      </td>
    </tr>
    <tr>
      <td width="260">Apellidos</td>
      <td width="340">
        <input asp-for="Personas.Apellido" class="form-control" size="35" />
      </td>
    <tr>
      <td width="260">Direccion</td>
      <td width="340">
        <input asp-for="Personas.Direccion" class="form-control" size="35" />
      </td>
    </tr>
    <tr>
      <td width="260">Corre Electronico</td>
      <td width="340">
        <input asp-for="Personas.Email" class="form-control" size="35" />
      </td>
    </tr>
    <tr>
      <td width="260">Campo adional 1</td>
      <td width="340">
        <input asp-for"Personas.campo" class="form-control" size="35" />
      </td>
    </tr>  
    <tr>
      <td width="260">Campo adional 2</td>
      <td width="340">
        <input asp-for"Personas.campo2" class="form-control" size="35" />
      </td>
    </tr>
    <tr>
      <td width="260">Campo adional 3</td>
      <td width="340">
        <input asp-for"Personas.campo3" class="form-control" size="35" />
      </td>
    </tr>
    </table>
    <br>
    <div class="justify-content-center text-center">
      <input type="submit"  class="btn btn-success" name="Submit" value="Enviar Datos" />
    </div>
    <hr>
 </form>

