@page
@model MyApp.Namespace.CrearvehiculoModel
@{
}
<div class="justify-content-center text-center">
    <h2>Crear Vehiculo</h2>
    <br>
    <hr>
 </div>
<form method="post">
  <table class="table-hover" width="600" height="300"  align="center" asp-validation-summary="ModelOnly" class="text-danger" >
    <tr>
      <td width="260" style="font-size:0.4cm;">Tipo de Vehiculo</td>
      <td width="340">
       <input asp-for="vehiculo.Tipo" class="form-control" size="35" style="font-size:0.4cm;"/>
       <span asp-validation-for="vehiculo.Tipo" class="text-danger" style="font-size:0.4cm;"></span>
      </td>
    </tr>
    <tr>
      <td width="260" style="font-size:0.4cm;">Matricula</td>
      <td width="340">
        <input asp-for="vehiculo.Matricula" class="form-control" size="35" style="font-size:0.5cm;" />
        <span asp-validation-for="vehiculo.Matricula" class="text-danger" style="font-size:0.4cm;"></span>
      </td>
    <tr>
      <td width="260" style="font-size:0.4cm;">Modelo</td>
      <td width="340">
        <input asp-for="vehiculo.Modelo" class="form-control" size="35" style="font-size:0.4cm;" />
      </td>
    </tr>
    <tr>
      <td width="260" style="font-size:0.4cm;">Marca</td>
      <td width="340">
        <input asp-for="vehiculo.Marca" class="form-control" size="35"  style="font-size:0.4cm;"/>
        <span asp-validation-for="vehiculo.Marca" class="text-danger" style="font-size:0.4cm;"></span>
      </td>
    </tr>
    <tr>
      <td width="260"style="font-size:0.4cm;" >Fecha de Ingreso</td>
      <td width="340">
        <input asp-for="vehiculo.FechaIngreso" class="form-control" size="35" style="font-size:0.4cm;" />
        <span asp-validation-for="vehiculo.FechaIngreso" class="text-danger" style="font-size:0.4cm;"></span>
      </td>
    </tr>  
    <tr>
      <td width="260" style="font-size:0.4cm;">Tipo de Falla</td>
      <td width="340">
        <input asp-for="vehiculo.Tipo_de_falla" class="form-control" size="35" style="font-size:0.4cm;" />
        <span asp-validation-for="vehiculo.Tipo_de_falla" class="text-danger" style="font-size:0.4cm;"></span>
      </td>
    </tr>
    <tr>
      <td width="260" style="font-size:0.4cm;">----</td>
      <td width="340">
        
      </td>
    </tr>
    </table>
    <br>
    <div class="justify-content-center text-center">
      <input type="submit"  class="btn btn-success" name="Submit" value="Enviar Datos" />
    </div>
    <hr>
 </form>

