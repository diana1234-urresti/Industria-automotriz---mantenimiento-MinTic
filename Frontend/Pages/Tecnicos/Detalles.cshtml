@page
@model MyApp.Namespace.DetallestecnicoModel
@{
}
<div class="justify-content-center text-center">
    <h2>Detalles Tecnicos</h2></div>
    <br>
    <hr><br>

     <table class="table-hover" width="600" height="300"  align="center" asp-validation-summary="ModelOnly" class="text-danger" >
    <tr>
      <td width="260" style="font-size:0.4cm;">Nombre</td>
      <td width="340">@Html.DisplayFor(m =>m.Tecnicos.Nombre)
       <span asp-validation-for="Tecnicos.Nombre" class="text-danger" style="font-size:0.4cm;"></span>
      </td>
    </tr>
    <tr>
      <td width="260" style="font-size:0.4cm;">Apellidos</td>
      <td width="340">@Html.DisplayFor(m =>m.Tecnicos.Apellido)
       <span asp-validation-for="Tecnicos.Apellido" class="text-danger" style="font-size:0.4cm;"></span>
      </td>
    </tr>
    <tr>
      <td width="260" style="font-size:0.4cm;">Correo Electronico</td>
      <td width="340">@Html.DisplayFor(m =>m.Tecnicos.Email)
       <span asp-validation-for="Tecnicos.Email" class="text-danger" style="font-size:0.4cm;"></span>
      </td>
    </tr>
    <tr>
      <td width="260" style="font-size:0.4cm;">Fecha de Nacimiento</td>
      <td width="340">@Html.DisplayFor(m =>m.Tecnicos.FechaN)
       <span asp-validation-for="Tecnicos.FechaN" class="text-danger" style="font-size:0.4cm;"></span>
      </td>
    </tr>
    <tr>
      <td width="260" style="font-size:0.4cm;">Numero de Identificacion </td>
      <td width="340">@Html.DisplayFor(m =>m.Tecnicos.Identificacion)
       <span asp-validation-for="Tecnicos.Identificacion" class="text-danger" style="font-size:0.4cm;"></span>
      </td>
    </tr>
    <tr>
      <td width="260" style="font-size:0.4cm;">Telefono</td>
      <td width="340">@Html.DisplayFor(m =>m.Tecnicos.Telefono)
       <span asp-validation-for="Tecnicos.Telefono" class="text-danger" style="font-size:0.4cm;"></span>
      </td>
    </tr>
    <tr>
      <td width="260" style="font-size:0.4cm;">Sueldo</td>
      <td width="340">@Html.DisplayFor(m => m.Tecnicos.Sueldo)
       <span asp-validation-for="Tecnicos.Sueldo" class="text-danger" style="font-size:0.4cm;"></span>
      </td>
    </tr>
  </table>
 <br>
    <hr>
    <div class="justify-content-center text-left"><a asp-page="./listar">Ir a Gestion Tecnicos</a></div>