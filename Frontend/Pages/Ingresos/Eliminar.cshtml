@page
@model MyApp.Namespace.EliminarIngresoModel
@{
}
<div class="justify-content-center text-center">
    <h1>Eliminara la siguiente factura</h1>
    <hr><br>
    <div class="container">
  <div class="row">
    <div class="col" ><strong>Nombre de usuario</strong></div>
    <div class="col">@Html.DisplayFor(m => m.Ingresos.NameUsuario)</div>
        <div class="w-100"></div>
    <div class="col"><strong>Contraseña</strong></div>
    <div class="col"> @Html.DisplayFor(m => m.Ingresos.Contraseña)</div>
        <div class="w-100"></div>
    <div class="col"><strong>Tipo de Usuario</strong></div>
    <div class="col">@Html.DisplayFor(m => m.Ingresos.TipoUsuario)</div>
        <div class="w-100"></div><br>
    <hr>
    <form method="post" >
    <input type="hidden"  asp-for="Ingresos.Id_ingreso"/>    
    <input type="submit" value="Confirmar" class="btn btn-danger" asp-page="/Eliminar/OnPost"/>    
    </form>
</div>
<div class="justify-content-center text-left"><a asp-page="./Listar">Ir a Gestion de Ingresos</a></div>